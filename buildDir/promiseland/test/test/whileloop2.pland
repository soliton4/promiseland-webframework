var somefun = (par){
  var c = new Callback();
  setTimeout(c.callback, 100);
  *c.promise;
  return par + 1;
};

var fourfun = ()*{
  return 4;
}

return {
  fun: (){
    var a = 1;
    var i = 0;
    while (i < *fourfun()){
      i = *somefun(i);
      a = a + 1;
    };
    if (a == 5){
      return 4;
    };
    return 0;
  }
};
